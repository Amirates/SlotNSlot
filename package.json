{
    "name": "truffle-init-webpack",
    "version": "0.0.1",
    "description": "Frontend example using truffle v3",
    "scripts": {
        "lint": "eslint ./",
        "clean": "rm -rf dst && rm -rf dst-sls && rm -rf temp && rm -f dst.zip && mkdir dst",
        "build": "webpack --config webpack.prod.config.js -p",
        "build-sls": "tsc",
        "pack": "rm -f dst.zip && cp version dst-sls/version && cp sls/package.json dst-sls/package.json && cd dst-sls && npm install --production && zip -q -r ../dst.zip . ** && cd ..",
        "dev": "webpack-dev-server --content-base app/ --hot --progress --colors --host 0.0.0.0",
        "deploy": "node dst-sls/deploy/deploy.js",
        "deploy:prod": "npm run clean && npm run build && npm run build-sls && npm run deploy && npm run pack && serverless deploy -s prod && ts-node ./sls/deploy/helpers/expireCloudFrontCache",
        "precommit": "lint-staged",
        "prepush": "truffle compile"
    },
    "lint-staged": {
        "{app,__{tests,mocks}__}/**/*.{js,jsx,scss}": [
            "prettier --write --single-quote --trailing-comma all --print-width 120",
            "git add"
        ]
    },
    "author": "Douglas von Kohorn",
    "license": "MIT",
    "devDependencies": {
        "@types/fs-extra": "^3.0.3",
        "@types/node": "^8.0.7",
        "autoprefixer": "^7.1.1",
        "aws-sdk": "^2.80.0",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.0.0",
        "babel-loader": "^6.4.1",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
        "babel-plugin-transform-regenerator": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-env": "^1.5.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.24.1",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.26.4",
        "eslint": "^3.2.2",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-config-standard": "^6.2.1",
        "eslint-plugin-babel": "^4.1.1",
        "eslint-plugin-import": "^2.3.0",
        "eslint-plugin-jsx-a11y": "^5.0.3",
        "eslint-plugin-mocha": "^4.10.1",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^7.1.0",
        "eslint-plugin-standard": "^2.3.1",
        "fs": "0.0.1-security",
        "fs-extra": "^3.0.1",
        "html-webpack-plugin": "^2.28.0",
        "husky": "^0.14.1",
        "json-loader": "^0.5.4",
        "lint-staged": "^4.0.0",
        "node-sass": "^4.5.3",
        "postcss-flexbugs-fixes": "^3.0.0",
        "postcss-loader": "^2.0.6",
        "precss": "^1.4.0",
        "prettier": "^1.5.2",
        "raw-loader": "^0.5.1",
        "s3": "^4.4.0",
        "sass-loader": "^6.0.6",
        "serverless": "^1.16.1",
        "style-loader": "^0.13.2",
        "svg-inline-loader": "^0.7.1",
        "truffle": "^3.3.1",
        "truffle-config": "^1.0.0",
        "truffle-contract": "^1.1.11",
        "truffle-expect": "0.0.3",
        "ts-node": "^3.1.0",
        "typescript": "^2.4.1",
        "web3": "^0.18.4",
        "webpack": "^2.6.1",
        "webpack-dev-server": "^2.4.5"
    },
    "dependencies": {
        "axios": "^0.16.2",
        "babel-runtime": "^6.23.0",
        "bignumber.js": "^4.0.2",
        "history": "^4.6.2",
        "immutable": "^3.8.1",
        "is-ci": "^1.0.10",
        "lodash.throttle": "^4.1.1",
        "pixi.js": "^4.5.3",
        "process-nextick-args": "^1.0.7",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-ga": "^2.2.0",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-svg-inline": "^2.0.0",
